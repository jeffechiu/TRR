<p id="notice"><%= notice %></p>

<h1>Team Games</h1>

<table>
  <thead>
    <tr>
      <th>Team</th>
      <th>Game</th>
      <th>Kills</th>
      <th>Deaths</th>
      <th>Assists</th>
      <th>Fbkill</th>
      <th>Fbdeath</th>
      <th>Fbassist</th>
      <th>Drakes</th>
      <th>Herald</th>
      <th>Barons</th>
      <th>Elders</th>
      <th>Dmg</th>
      <th>Gold</th>
      <th>Goldspent</th>
      <th>Wardsplaced</th>
      <th>Wardskilled</th>
      <th>Viswards</th>
      <th>Viswardbuys</th>
      <th>Cs10</th>
      <th>Oppcs10</th>
      <th>Gold10</th>
      <th>Oppgold10</th>
      <th>Gold15</th>
      <th>Oppgold15</th>
      <th>Xp10</th>
      <th>Oppxp10</th>
      <th>Side</th>
      <th>Pick1</th>
      <th>Pick2</th>
