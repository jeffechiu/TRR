<p id="notice"><%= notice %></p>

<h1>Player Games</h1>

<table>
  <thead>
    <tr>
      <th>Player</th>
      <th>Game</th>
      <th>Champion</th>
      <th>Kills</th>
      <th>Deaths</th>
      <th>Assists</th>
      <th>Doubles</th>
      <th>Triples</th>
      <th>Quadras</th>
      <th>Pentas</th>
      <th>Fbkill</th>
      <th>Fbdeath</th>
      <th>Fbassist</th>
      <th>Drakes</th>
      <th>Herald</th>
      <th>Barons</th>
      <th>Dmg</th>
      <th>Gold</th>
      <th>Goldspent</th>
      <th>Wardsplaced</th>
      <th>Wardskilled</th>
      <th>Viswards</th>
      <th>Viswardbuys</th>
      <th>Cs10</th>
      <th>Oppcs10</th>
      <th>Gold10</th>
      <th>Oppgold10</th>
      <th>Gold15</th>
      <th>Oppgold15</th>
      <th>Xp10</th>
      <th>Oppxp10</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @player_games.each do |player_game| %>
      <tr>
        <td><%= player_game.player %></td>
        <td><%= player_game.game %></td>
        <td><%= player_game.champion %></td>
        <td><%= player_game.kills %></td>
        <td><%= player_game.deaths %></td>
        <td><%= player_game.assists %></td>
        <td><%= player_game.doubles %></td>
        <td><%= player_game.triples %></td>
        <td><%= player_game.quadras %></td>
        <td><%= player_game.pentas %></td>
        <td><%= player_game.fbkill %></td>
        <td><%= player_game.fbdeath %></td>
        <td><%= player_game.fbassist %></td>
        <td><%= player_game.drakes %></td>
        <td><%= player_game.herald %></td>
        <td><%= player_game.barons %></td>
        <td><%= player_game.dmg %></td>
        <td><%= player_game.gold %></td>
        <td><%= player_game.goldspent %></td>
        <td><%= player_game.wardsplaced %></td>
        <td><%= player_game.wardskilled %></td>
        <td><%= player_game.viswards %></td>
        <td><%= player_game.viswardbuys %></td>
        <td><%= player_game.cs10 %></td>
        <td><%= player_game.oppcs10 %></td>
        <td><%= player_game.gold10 %></td>
        <td><%= player_game.oppgold10 %></td>
        <td><%= player_game.gold15 %></td>
        <td><%= player_game.oppgold15 %></td>
        <td><%= player_game.xp10 %></td>
        <td><%= player_game.oppxp10 %></td>
        <td><%= link_to 'Show', player_game %></td>
        <td><%= link_to 'Edit', edit_player_game_path(player_game) %></td>
        <td><%= link_to 'Destroy', player_game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Player Game', new_player_game_path %>
